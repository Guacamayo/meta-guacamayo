export GUACABASE="`pwd`"
export TEMPLATECONF="$GUACABASE/meta-guacamole/conf"
export BDIR=build
export BUILDDIR="$GUACABASE/build"
HAVE_BBLAYERS=

echo "TEMPLATECONF=$TEMPLATECONF"

if (test -r "$BUILDDIR/conf/bblayers.conf"); then
   HAVE_BBLAYERS=yes
fi

source "$GUACABASE/upstream/yocto/oe-init-build-env"

if (test x$HAVE_BBLAYERS = x); then
    sed -i "s|##GUACABASE##|$GUACABASE|g" $BUILDDIR/conf/bblayers.conf
fi
