-			 0x11E); /* CAM_D4 */
-	omap_ctrl_writew(OMAP_PIN_INPUT | OMAP_MUX_MODE0,
-			 0x120); /* CAM_D5 */
-	omap_ctrl_writew(OMAP_PIN_INPUT | OMAP_MUX_MODE0,
-			 0x122); /* CAM_D6 */
-	omap_ctrl_writew(OMAP_PIN_INPUT | OMAP_MUX_MODE0,
-			 0x124); /* CAM_D7 */
-	omap_ctrl_writew(OMAP_PIN_INPUT | OMAP_MUX_MODE0,
-			 0x126); /* CAM_D8 */
-	omap_ctrl_writew(OMAP_PIN_INPUT | OMAP_MUX_MODE0,
