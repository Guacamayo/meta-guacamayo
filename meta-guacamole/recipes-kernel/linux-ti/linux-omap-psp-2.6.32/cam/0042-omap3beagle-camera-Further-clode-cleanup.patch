-	omap_ctrl_writew(OMAP_PIN_INPUT | OMAP_MUX_MODE0,
-			 0x12A); /* CAM_D10 */
-	omap_ctrl_writew(OMAP_PIN_INPUT | OMAP_MUX_MODE0,
-			 0x12C); /* CAM_D11 */
-
 	return 0;
 }
 
